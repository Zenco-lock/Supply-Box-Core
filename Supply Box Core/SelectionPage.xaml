<Page x:Class="Supply_Box_Core.SelectionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Supply_Box_Core"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SelectionPage">

    <Grid Background="DarkSlateGray">
        <TextBlock Text="SelectionPage" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="White" Background="Black" FontFamily="Bahnschrift SemiBold" />
        <Button x:Name="PasswordManager" Content="Password&#xD;&#xA;Manager" Width="200" Height="100" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,50,0,0" Foreground="White" Background="Black" Click="Button_Click" FontSize="22" FontFamily="Bahnschrift SemiBold" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Button x:Name="TBA1" Content="TBA" Width="200" Height="100" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="100,0,0,50" Foreground="White" Background="Gray" FontFamily="Bahnschrift SemiBold" FontSize="22" Click="Button_Click_1" />
        <Button x:Name="TBA2" Content="TBA" Width="200" Height="100" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,100,50" Foreground="Black" Background="White" FontSize="22" FontFamily="Bahnschrift SemiBold" Click="Button_Click_2"/>
    </Grid>

</Page>
